import pandas as pd
import statsmodels.api as sm


df = pd.read_excel(r"C:\Users\Zdeněk Beránek\Desktop\phytonsoubory\Predikce-HDP\Regrese_data (1).xlsx")


print(df.head())
print(df.columns.tolist())



y = df["Hdp"]  


X = df[[
    "Zahraniční obchod",
    "Průmysl",
    "Nezaměstnanost",
    "Inflace",       
    "Mzda"
]]


X = sm.add_constant(X)


model = sm.OLS(y, X).fit()


print(model.summary())
